{
  "name": "turbo-pnpm-example",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "chalk": "5.0.1",
    "eslint-config-custom": "workspace:*",
    "prettier": "2.7.0",
    "turbo": "1.2.16"
  },
  "engines": {
    "pnpm": "7.x",
    "node": "16.x"
  },
  "packageManager": "pnpm@7.2.0",
  "volta": {
    "node": "16.15.1"
  },
  "dependencies": {
    "inquirer": "8.2.4",
    "moment": "2.29.3"
  }
}
